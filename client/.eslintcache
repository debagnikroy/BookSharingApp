\"\r\n            placeholder=\"Create Password\"\r\n            value={password}\r\n            required\r\n\t\t\tonChange={handleChange('password')}\r\n          />\r\n        </div>\r\n        \r\n        <button onClick={clickSubmit} className=\"btn btn-primary btn-block\">\r\n          Register\r\n        </button>\r\n      </form>\r\n      <p className=\"lead mt-4\">Have An Account? <Link to=\"/signin\">Login</Link></p>\r\n    </div>\r\n  </div>\r\n</div>\r\n    );\r\n\r\n    const showError = () => (\r\n        <div className=\"alert alert-danger\" style={{ display: error ? '' : 'none' }}>\r\n            {error}\r\n        </div>\r\n    );\r\n\r\n    const showSuccess = () => (\r\n        <div className=\"alert alert-info\" style={{ display: success ? '' : 'none' }}>\r\n            New account is created. Please <Link to=\"/signin\">Signin</Link>\r\n        </div>\r\n    );\r\n\r\n    return (\r\n        <Layout\r\n            title=\"Signup\"\r\n            description=\"Signup to Node React E-commerce App\"\r\n            className=\"container col-md-8 offset-md-2\"\r\n        >\r\n            {isAuthenticated()?<Redirect to=\"/user/dashboard\" /> :null}\r\n            {showSuccess()}\r\n            {showError()}\r\n            {signUpForm()}\r\n        </Layout>\r\n    );\r\n};\r\n\r\nexport default Signup;\r\n","C:\\Users\\debag\\OneDrive\\Desktop\\finalProj\\booksharingapp23\\client\\src\\user\\Signin.js",["76","77"],"import React, { useEffect, useState } from 'react';\r\nimport {Link, Redirect} from 'react-router-dom';\r\nimport {signin,authenticate,isAuthenticated} from '../auth';\r\nimport Layout from \"../core/Layout\";\r\n\r\n\r\nconst Signin = () =>{\r\n    const [values,setValues]=useState({\r\n        name:'',\r\n        email:'',\r\n        password:'',\r\n        error:'',\r\n        loading:false,\r\n        redirectToReferrer:false \r\n    });\r\n\r\n    const {email,password,error,loading,redirectToReferrer}=values;\r\n    const {user}=isAuthenticated();\r\n\r\n    const clickSubmit=(event)=>{\r\n        event.preventDefault();\r\n        setValues({...values, loading:true, error:false});\r\n        signin({ email, password })\r\n        .then(data => {\r\n            if (data.error) {\r\n                setValues({ ...values, error: data.error, loading: false });\r\n            } else {\r\n                authenticate(data,()=>{\r\n                    setValues({\r\n                        ...values,                        \r\n                        redirectToReferrer: true\r\n                    });\r\n                })\r\n            }\r\n        });          \r\n    }\r\n\r\n    \r\n    const handleChange=name=>event=>{\r\n        setValues({...values,error:false,[name]:event.target.value});\r\n    };\r\n    const signUpForm=()=>(\r\n        <div className=\"row mt-5\">\r\n  <div className=\"col-md-6 m-auto\">\r\n    <div className=\"card card-body\">\r\n      <h1 className=\"text-center mb-3\">Login</h1>\r\n      <form>\r\n        <div className=\"form-group\">\r\n          <label for=\"email\">Email</label>\r\n          <input\r\n            type=\"email\"\r\n            id=\"email\"\r\n            name=\"email\"\r\n            className=\"form-control\"\r\n            placeholder=\"Enter Email\"\r\n\t\t\tonChange={handleChange('email')}\r\n          />\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label for=\"password\">Password</label>\r\n          <input\r\n            type=\"password\"\r\n            id=\"password\"\r\n            name=\"password\"\r\n            className=\"form-control\"\r\n            placeholder=\"Enter Password\"\r\n\t\t\tonChange={handleChange('password')}\r\n          />\r\n        </div>\r\n        <button onClick={clickSubmit} className=\"btn btn-primary btn-block\">Login</button>\r\n      </form>\r\n      <p className=\"lead mt-4\">\r\n        No Account? <Link to=\"/signup\">Signup here</Link>\r\n      </p>\r\n    </div>\r\n  </div>\r\